{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "noImplicitAny": true
  },
  "exclude": [
    "node_modules"
  ]
}  